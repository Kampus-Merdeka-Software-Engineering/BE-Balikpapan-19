// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Pengiriman {
  id Int @id @default(autoincrement())
  kota_asal String
  kota_tujuan String
  berat_kg Int
  price Int
}

model Lacak {
  id Int @id @default(autoincrement())
  nama String
  numberTelpon Int
  statusPengiriman String
}